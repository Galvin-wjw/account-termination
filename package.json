{
  "name": "aws-account-termination",
  "version": "1.0.0",
  "description": "AWS Account Termination Solution using CDK and Step Functions",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:property": "jest --testPathPattern=property",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy --context environment=dev",
    "deploy:staging": "cdk deploy --context environment=staging",
    "deploy:prod": "cdk deploy --context environment=prod",
    "destroy": "cdk destroy",
    "synth": "cdk synth",
    "synth:dev": "cdk synth --context environment=dev",
    "synth:staging": "cdk synth --context environment=staging",
    "synth:prod": "cdk synth --context environment=prod",
    "diff": "cdk diff",
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "validate": "npm run build && npm run lint && npm test",
    "clean": "rm -rf lib cdk.out node_modules/.cache",
    "docs": "typedoc --out docs src",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix"
  },
  "keywords": [
    "aws",
    "cdk",
    "step-functions",
    "lambda",
    "account-termination"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "aws-cdk": "^2.108.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.108.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1498.0",
    "constructs": "^10.3.0"
  }
}